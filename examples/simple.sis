% This file contains some simple sisal code for testing purposes

define Main

function test(returns integer)
	let 
		a := 
		if 1 < 2 then 1 + 2 else 3 + 4
			%then if True then 1 + 2 else 10 + 20 end if
			%else 3 + 4 
		end if;
		b := 
			if 1 > 2 
				then 1 + 2 
				else 3 + 4 
			end if
	in
		a + b
	end let
end function


function main(returns integer)
	test()
end function