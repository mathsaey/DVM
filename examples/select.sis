% select.sis
% Mathijs Saey
% dvm examples

% This file contains some simple sisal code for testing the select node

define Main

function test(returns integer)
	let 
		a :=
		if 1 < 2
			then 1 + 2
			else 3 + 4 
		end if;
		b := 
			if 1 > 2 
				then 1 + 2 
				else 3 + 4 
			end if
	in
		a + b
	end let
end function


function main(returns integer)
	test()
end function